{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "task-manager-ui-test",
  "version": "0.1.0",
  "identifier": "com.lielfr.task-manager-ui.test",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "task-manager-ui-test",
        "width": 1280,
        "height": 1024,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
